# Database
MONGO_URI=mongodb://localhost:27017/your-database-name

# Server
PORT=4000
NODE_ENV=development

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
REFRESH_TOKEN_SECRET=your-refresh-token-secret-key-change-this-in-production

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Frontend
FRONTEND_URL=http://localhost:3000

# Google Safe Browsing API
GOOGLE_SB_API=your-google-safe-browsing-api-key

# Cloudinary
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_URL=cloudinary://api_key:api_secret@cloud_name
PUBLIC_CLOUDINARY_UPLOAD_ENDPOINT=https://api.cloudinary.com/v1_1/your-cloud-name/image/upload
